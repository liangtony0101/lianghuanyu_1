   "source": [
    "import pyinputplus as pyip\n",
    "\n",
    "num = pyip.inputInt(\"請輸入整數,求所有因數:\")\n",
    "print(num)\n",
    "print(f\"{num}的因數有:\",end=\"\")\n",
    "for i in range(1,num+1):\n",
    "    a = num % i\n",
    "    if a == 0:\n",
    "        print(i,end=\",\")"
   ]
  }
