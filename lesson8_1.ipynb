   "source": [
    "import math\n",
    "# 1個參數,有傳出值\n",
    "def circule_area(radius):\n",
    "    area = radius ** 2 * math.pi\n",
    "    return area"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
